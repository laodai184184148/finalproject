 <div id="carouselExampleIndicators" class="carousel slide my-4" data-ride="carousel" >
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" ></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="carousel-item active">
            <img class="d-block img-fluid" src="/img/di-cho-gium-ban-giao-tan-nha-17042020112016.jpg" alt="First slide">
        </div>
        <div class="carousel-item">
            <img class="d-block img-fluid" src="/img/mua-ca-phe-boss-tang-oolong-12052020101455.jpg" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block img-fluid" src="/img/woods.jpg" alt="Third slide">
        </div>
    </div>
    <div class="tav_menu_wrapper" style="margin-top: 10px;">
        <div class="row align-items-center">
            <div class="col-lg-6 col-md-7 col-sm-6">
                <div class="tab_menu shop_menu">
                    
                    <div class="tab_menu_right">    
                        <%if (typeof key != 'undefined') { %>
                        <p>Kết Quả Tìm kiếm cho : <%= key%></p>
                        <% }  %>
                         
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-5 col-sm-6">
                <div class="    ">
                    <div class="dropdown dropdown-shop">
                        <select name="drop" id="dropdown">
                            <option value="1">Sự liên quan</option>   
                            <option value="2">Tên, từ A đến Z</option>
                            <option value="2">Tên, Z đến A</option>
                            <option value="2">Giá, thấp đến cao</option>
                            <option value="2">Giá từ cao đến thấp</option>
                        </select>
                    </div>
                </div>    
            </div>    
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>